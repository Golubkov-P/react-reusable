{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": ["airbnb", "airbnb/hooks"],
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "import/prefer-default-export": 0,
    "import/extensions": 0,
    "react-hooks/exhaustive-deps": 0,
    "consistent-return": 0,
    "no-unused-vars": 0,
    "@typescript-eslint/no-unused-vars": ["error"]
  },
  "env": {
    "browser": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".ts",
          ".tsx"
        ]
      }
    }
  }
}
