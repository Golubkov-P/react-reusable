{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": ["airbnb", "airbnb/hooks"],
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "import/prefer-default-export": 0,
    "import/extensions": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".ts",
          ".tsx"
        ]
      }
    }
  }
}
